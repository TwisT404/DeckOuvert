{
  "expo": {
    "name": "DeckOuvert",
    "slug": "deckouvert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_app_icon_resized.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_app_icon_resized.png",
        "backgroundColor": "#07091A"
      },
      "package": "com.twist404.deckouvert"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_app_icon_resized.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3b409616-ba41-431c-bc11-ae3a736a03da"
      }
    },
    "owner": "twist404"
  }
}
